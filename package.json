{
  "name": "blog-server",
  "version": "1.0.0",
  "description": "博客API服务",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "ali-oss": "^6.13.1",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.2.0",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "egg-sequelize-auto": "^0.1.1",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "svg-captcha": "^1.4.0",
    "ua-parser-js": "^0.7.23"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-diary",
    "start-pro": "egg-scripts start --daemon --title=egg-server-diary --ignore-stderr",
    "stop": "egg-scripts stop --title=egg-server-blog-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "dbload": "egg-sequelize-auto -o ./app/model -h ip -p port -d database -u account -x password"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "yemingrujing",
  "license": "MIT"
}
